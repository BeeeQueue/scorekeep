{
  "name": "@scorekeep/server",
  "version": "0.0.0",
  "author": "BeeeQueue",
  "dependencies": {
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.2",
    "@types/helmet": "0.0.45",
    "@types/superagent": "4.1.4",
    "@types/uuid": "3.4.6",
    "ajv": "6.10.2",
    "apollo-server-express": "2.9.7",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "date-fns": "2.6.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "googleapis": "44.0.0",
    "graphql": "14.5.8",
    "graphql-tools": "4.0.6",
    "graphql-type-json": "0.3.0",
    "helmet": "3.21.2",
    "pg": "7.12.1",
    "reflect-metadata": "0.1.13",
    "superagent": "5.1.0",
    "ts-node": "8.4.1",
    "ts-optchain": "0.1.8",
    "tsconfig-paths": "3.9.0",
    "type-graphql": "0.17.5",
    "typeorm": "0.2.20",
    "uuid": "3.3.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/graphql-type-json": "0.3.2",
    "@types/type-is": "1.6.3",
    "ts-node-dev": "1.0.0-pre.44"
  },
  "engines": {
    "node": ">=12.9.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "repository": "https://github.com/BeeeQueue/scorekeep",
  "scripts": {
    "build": "yarn tsc --noEmit -P tsconfig.json",
    "dev": "ts-node-dev --respawn --no-deps -r dotenv/config -r tsconfig-paths/register --inspect -- src/index.ts",
    "lint": "eslint 'src/**/*.ts'",
    "migrate:create": "typeorm migration:create -d migrations -n",
    "start": "ts-node -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "test": "jest server -i",
    "test:coverage": "yarn run test --coverage"
  }
}
