{
  "name": "scorekeep",
  "description": "Easily set up recurring payments based on plans.",
  "version": "0.0.0",
  "author": "BeeeQueue",
  "dependencies": {
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.0",
    "@types/helmet": "0.0.44",
    "@types/node": "12.7.3",
    "@types/superagent": "4.1.3",
    "@types/uuid": "3.4.5",
    "apollo-server-express": "2.9.3",
    "cookie-parser": "1.4.4",
    "date-fns": "2.0.1",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "googleapis": "42.0.0",
    "graphql": "14.5.4",
    "graphql-tools": "4.0.5",
    "helmet": "3.20.1",
    "pg": "7.12.1",
    "reflect-metadata": "0.1.13",
    "superagent": "5.1.0",
    "ts-node": "8.3.0",
    "ts-optchain": "0.1.8",
    "tsconfig-paths": "3.8.0",
    "type-graphql": "0.17.5",
    "typeorm": "0.2.18",
    "typescript": "3.6.2",
    "uuid": "3.3.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@typescript-eslint/eslint-plugin": "2.1.0",
    "@typescript-eslint/parser": "2.1.0",
    "coveralls": "3.0.6",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.2.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "9.2.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "lint-staged": "9.2.5",
    "prettier": "1.18.2",
    "sqlite3": "4.1.0",
    "ts-jest": "24.0.2",
    "ts-node-dev": "1.0.0-pre.42"
  },
  "engines": {
    "node": ">=12.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "AGPL-3.0-only",
  "lint-staged": {
    "*.{js,ts,tsx,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "private": true,
  "repository": "https://github.com/BeeeQueue/scorekeep",
  "scripts": {
    "build": "yarn tsc --noEmit -P tsconfig.json",
    "dev": "ts-node-dev --respawn -r dotenv/config -r tsconfig-paths/register --inspect -- src/index.ts",
    "lint": "eslint 'src/**/*'",
    "migrate:create": "typeorm migration:create -d migrations -n",
    "pretest": "NODE_ENV=test yarn run migrate",
    "start": "ts-node -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "test": "jest -i",
    "test:coverage": "yarn run test --coverage"
  }
}
